{
  "name": "@vjscc/back-top",
  "version": "1.0.0",
  "description": "Vanilla JavaScript back-to-top component",
  "license": "MIT",
  "author": {
    "name": "Gu-Miao",
    "email": "1450206741@qq.com",
    "url": "https://github.com/Gu-Miao"
  },
  "keywords": [
    "vjscc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gu-Miao/vjscc.git"
  },
  "homepage": "https://vjscc.gutianhuang.red",
  "bugs": {
    "url": "https://github.com/Gu-Miao/vjscc/issues"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": "src/index.less",
  "scripts": {
    "dev": "rimraf dist && father dev",
    "e2e": "wait-on dist/esm/index.js && live-server --open=e2e --watch=e2e,dist",
    "start": "concurrently \"pnpm dev\" \"pnpm e2e\"",
    "start:back-top": "pnpm start",
    "build": "rimraf dist && father build",
    "lint": "eslint src --max-warnings 0 && father doctor"
  },
  "peerDependencies": {
    "@vjscc/utils": "latest"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.11",
    "@vjscc/utils": "workspace:*"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "father": "^4.3.1",
    "less": "^4.1.3",
    "live-server": "^1.2.2",
    "rimraf": "^4.1.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "wait-on": "^7.0.1"
  }
}
